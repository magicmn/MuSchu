<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <!-- Wicket Defaults die überschrieben werden -->
    <entry key="Required">${label} darf nicht leer sein.</entry>
    <!-- Pagetitles -->
    <entry key="Home">Startseite</entry>
    <entry key="Anmeldung">Anmeldung</entry>
    <!-- Login -->
    <entry key="Login">Anmeldung</entry>
    <entry key="Login.nutzername.label">Nutzername</entry>
    <entry key="Login.nutzername.hint">mmuster</entry>
    <entry key="Login.nutzername.help">&lt;a tabindex="0" data-toggle="popover" data-trigger="focus" data-content="Ihr Nutzernamen entspricht Ihrem Vornamen und falls ihr Name mehrfach vorhanden ist einem nummerischen Suffix."&gt;Welcher Nutzername?&lt;/a&gt;.</entry>
    <entry key="Login.passwort.label">Passwort</entry>
    <entry key="Login.passwort.hint">12345</entry>
    <entry key="Login.submit">Einloggen</entry>
    <entry key="Login.failed">Ungültige Kombination aus Nutzername und Passwort.</entry>
    <!-- Account -->
    <entry key="Account.info">Accountinformationen</entry>
    <entry key="Account.welcome">Hallo ${nutzername}!</entry>
    <entry key="Logout">Abmelden</entry>
    <!-- Stunde -->
    <entry key="Stunde.woche">Stunden diese Woche</entry>
    <entry key="Stunde.total">Alle Stunden</entry>
    <entry key="Stunde.inhalt.label">Inhalt der Stunde: </entry>
    <entry key="Stunde.raum.null">Die Stunde hat noch keinen Raum zugewiesen bekommen!</entry>
    <!-- Schueler -->
    <entry key="Schueler.nachname">Nachname</entry>
    <entry key="Schueler.vorname">Vorname</entry>
    <entry key="Schueler.geburtsdatum">Geburtsdatum</entry>
    <entry key="Schueler.postleitzahl">Postleitzahl</entry>
    <entry key="Schueler.ort">Wohnort</entry>
    <entry key="Schueler.strasse">Straße</entry>
    <entry key="Schueler.telefonnummer">Telefonnummer</entry>
    <entry key="Schueler.email">Mail-Adresse</entry>
    <!-- Validators -->
    <!-- LengthValidator -->
    <entry key="LengthValidator.min">${label} muss aus mindestens ${min} Zeichen bestehen.</entry>
    <entry key="LengthValidator.max">${label} darf maxmimal ${max} Zeichen lang sein.</entry>
    <entry key="LengthValidator.Max">${label} muss zwischen ${mmin} und ${max} Zeichen haben.</entry>
</properties>